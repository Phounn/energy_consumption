generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model measurement_log {
  measureId       String @id @unique 
  voltage         Float
  current         Float 
  power           Float
  createdTime     DateTime @default(now())
}

model user {
  id       String @id @unique
  name     String
  email    String
  password String
}